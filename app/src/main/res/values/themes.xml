<resources>
    <!-- Base application theme. -->
    <style name="Theme.地铁族" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue5</item>
        <item name="colorPrimaryVariant">@color/blue7</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue2</item>
        <item name="colorSecondaryVariant">@color/blue7</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/blue2</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:textColor">@color/textPrimary</item>
    </style>
</resources>